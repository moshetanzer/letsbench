{
  "name": "letsbench",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@10.11.0",
  "description": "A CLI tool to benchmark and compare functions from different NPM packages with detailed performance metrics and visual feedback.",
  "license": "MIT",
  "repository": "https://github.com/moshetanzer/letsbench",
  "keywords": [
    "benchmark",
    "cli",
    "performance",
    "function-comparison",
    "npm",
    "node",
    "typescript",
    "benchmarking-tool",
    "performance-testing",
    "head-to-head",
    "function-benchmark",
    "compare-libraries",
    "nodejs",
    "performance-metrics"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.mts"
      ]
    }
  },
  "bin": {
    "letsbench": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "pnpm test:unit && pnpm test:types",
    "test:unit": "vitest",
    "test:knip": "knip",
    "test:versions": "installed-check -d --no-workspaces",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@types/figlet": "^1.7.0",
    "@types/minimist": "^1.2.5",
    "chalk": "^5.4.1",
    "figlet": "^1.8.1",
    "inquirer": "^12.6.1",
    "minimist": "^1.2.8",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@types/node": "^22.15.21",
    "bumpp": "latest",
    "changelogithub": "latest",
    "eslint": "latest",
    "installed-check": "latest",
    "knip": "latest",
    "lint-staged": "latest",
    "pkg-pr-new": "^0.0.50",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  },
  "resolutions": {
    "package-name": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  }
}
